<?xml version="1.0" encoding="UTF-8"?>
<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="findbugsplugin.xsd"
        pluginid="com.tmh.bugdroid">

        <Detector class="com.tmh.bugdroid.webview.JavascriptEnabled" reports="WEB_VIEW_JAVASCRIPT_ENABLED" ></Detector>
        <Detector class="com.tmh.bugdroid.webview.FileAccess" reports="WEBVIEW_UNIVERSAL_FILE_ACCESS" ></Detector>
        <Detector class="com.tmh.bugdroid.webview.JavascriptInterface" reports="WEB_VIEW_JAVASCRIPT_INTERFACE_ADDED"></Detector>
        <Detector class="com.tmh.bugdroid.files.ExternalFilesDirectory" reports="EXTERNAL_FILES_DIR_USED" ></Detector>        
        <Detector class="com.tmh.bugdroid.broadcast.RegisteredReceiver" reports="RECEIVER_REGISTERED" ></Detector>
        <Detector class="com.tmh.bugdroid.broadcast.SendBroadcast" reports="BROADCAST_SENT" ></Detector>        
        <Detector class="com.tmh.bugdroid.files.OverwriteFiles" reports="INTERCEPT_FILE_INTENT" ></Detector>     
        <Detector class="com.tmh.bugdroid.arbitraryaccess.ContentProviderAccess" reports="CONTENT_PROVIDER_ACCESS" ></Detector>
        <Detector class="com.tmh.bugdroid.arbitraryaccess.InstallApk" reports="INSTALL_ARBITRARY_APK" ></Detector>      
        <Detector class="com.tmh.bugdroid.arbitraryaccess.SharedPrefs" reports="SHARED_PREFS_READ" ></Detector>      
   
	<BugPattern abbrev="BDJSE" category="SECURITY" type="WEB_VIEW_JAVASCRIPT_ENABLED" />
	<BugPattern abbrev="BDFA" category="SECURITY" type="WEBVIEW_UNIVERSAL_FILE_ACCESS" />
	<BugPattern abbrev="BDJSI" category="SECURITY" type="WEB_VIEW_JAVASCRIPT_INTERFACE_ADDED" />
	<BugPattern abbrev="BDEFD" category="SECURITY" type="EXTERNAL_FILES_DIR_USED" />
	<BugPattern abbrev="BDRR" category="SECURITY" type="RECEIVER_REGISTERED" />
	<BugPattern abbrev="BDBS" category="SECURITY" type="BROADCAST_SENT" />
	<BugPattern abbrev="BDFI" category="SECURITY" type="INTERCEPT_FILE_INTENT" />
	<BugPattern abbrev="BDIA" category="SECURITY" type="INSTALL_ARBITRARY_APK" />
	<BugPattern abbrev="BDSPR" category="SECURITY" type="SHARED_PREFS_READ" />
	
</FindbugsPlugin>
